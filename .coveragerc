[run]
source = api
omit =
    */migrations/*
    */tests/*
    */venv/*
    */__pycache__/*
    */config.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
show_missing = true
precision = 2

[xml]
output = coverage.xml